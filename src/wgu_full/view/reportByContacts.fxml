<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wgu_full.controller.ReportByContactsController">
   <children>
      <Label alignment="CENTER" layoutX="9.0" layoutY="32.0" prefHeight="47.0" prefWidth="884.0" text="Appointments by Contacts">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="contactReportTable" layoutX="25.0" layoutY="147.0" prefHeight="348.0" prefWidth="850.0">
        <columns>
          <TableColumn fx:id="apptIdCol" prefWidth="43.0" text="Id" />
            <TableColumn fx:id="titleCol" prefWidth="98.0" text="Title" />
            <TableColumn fx:id="descriptionCol" prefWidth="131.0" text="Description" />
            <TableColumn fx:id="locationCol" prefWidth="111.0" text="Location" />
            <TableColumn fx:id="typeCol" prefWidth="105.0" text="Type" />
            <TableColumn fx:id="startCol" prefWidth="108.0" text="Start" />
            <TableColumn fx:id="endCol" prefWidth="123.0" text="End" />
            <TableColumn fx:id="userIdCol" prefWidth="51.0" text="User_ID" />
            <TableColumn fx:id="customerCol" prefWidth="79.0" text="Customer_ID" />
        </columns>
      </TableView>
      <Button fx:id="cancelBtn" layoutX="668.0" layoutY="526.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="65.0" text="Cancel" />
      <Label layoutX="654.0" layoutY="109.0" prefHeight="17.0" prefWidth="65.0" text="Contact: ">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Label>
      <Button fx:id="reportBtn" layoutX="748.0" layoutY="526.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="127.0" text="Generate Report" />
      <ComboBox fx:id="contactCombo" layoutX="725.0" layoutY="106.0" prefWidth="150.0" promptText="Select one ..." />
      <Separator layoutX="258.0" layoutY="85.0" prefHeight="4.0" prefWidth="387.0" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="33.0" layoutY="526.0" prefHeight="32.0" prefWidth="610.0" text="ERROR" textFill="#ff4e4e" visible="false">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
